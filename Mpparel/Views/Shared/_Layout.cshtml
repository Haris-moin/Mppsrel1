    <script>
        var menu = new Vue({
            el: "#menu",
            data: {
                categories: [],
                category_count: 0,
                nested_categories: []
            },
            created: function () {
                this.getJson();
            },
            methods: {
                getJson: function () {
                    var self = this;
                    $.getJSON('http://52.71.235.183:5000/categories', function (json) {
                        self.categories = json;
                        self.category_count = self.categories.length;



                        var map = {};
                        for (var i = 0; i < self.category_count; i++) {
                            var obj = self.categories[i];
                            obj.items = [];

                            map[obj.id] = obj;

                            var parent = obj.parent_id[0] || '-';
                            if (!map[parent]) {
                                map[parent] = {
                                    items: []
                                };
                            }
                            map[parent].items.push(obj);
                        }

                        self.nested_categories = map['-'].items;

                    });
                }
            }
        });

        var cart = new Vue({
            el: '#cart',
            data: {
                products: [],
                counter: 0,
            },
            created: function () {
                this.total();
            },
            methods: {
                total: function () {
                    this.products = JSON.parse(localStorage.getItem('cart')) || [];
                    this.counter = this.products.length;
                },
                subtractFromCart: function (p) {

                    var items = JSON.parse(localStorage.getItem('cart')) || [];
                    var index = items.map(function (x) { return x.value.id; }).indexOf(p.id);
                    if (index > -1) {
                        items.splice(index, 1);
                    }
                    localStorage.setItem('cart', JSON.stringify(items));
                    cart.total();
                }
            }
        });

    </script>

